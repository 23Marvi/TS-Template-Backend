{
	"name": "ts-bemplate-backend",
	"version": "1.0.0",
	"description": "A template for a backend project using TypeScript",
	"main": "./build/main/index.js",
	"scripts": {
		"start:ci": "node ./build/main/index.js",
		"start": "nodemon ./build/main/index.js",
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "eslint . --ext .ts",
		"lint:json": "eslint . --ext .ts -f json -o .eslint-results.json",
		"lint:fix": "eslint . --fix --ext .ts",
		"sonar": "sonar-scanner",
		"prestart": "copyfiles -f src/assets/* build/assets",
		"postbuild": "copyfiles -f src/assets/* build/assets",
		"test": "PORT=1337 start-server-and-test start:ci http://127.0.0.1:1337/documentation test:ci",
		"test:ci": "nyc mocha --require ts-node/register --extension spec.ts --recursive ./src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/23Marvi/TS-Template-Backend"
	},
	"bugs": {
		"url": "https://github.com/23Marvi/TS-Template-Backend/issues"
	},
	"author": "Marten",
	"license": "ISC",
	"dependencies": {
		"@types/webrtc": "0.0.33",
		"bcrypt": "5.1.0",
		"bignumber.js": "9.1.1",
		"copyfiles": "2.4.1",
		"cors": "2.8.5",
		"express": "4.18.2",
		"express-http-proxy": "1.6.3",
		"jsonwebtoken": "8.5.1",
		"moment": "2.29.4",
		"nodemon": "2.0.20",
		"pg": "8.8.0",
		"reflect-metadata": "0.1.13",
		"sequelize": "6.26.0",
		"sequelize-cli": "6.5.2",
		"sequelize-typescript": "2.1.5",
		"superagent": "8.0.5",
		"swagger-jsdoc": "6.2.5",
		"swagger-ui-express": "4.6.0",
		"ts-node": "10.9.1",
		"umzug": "3.2.1",
		"uuidv4": "6.2.13",
		"web3": "1.8.1"
	},
	"devDependencies": {
		"@types/bcrypt": "5.0.0",
		"@types/chai": "4.3.4",
		"@types/cors": "2.8.13",
		"@types/express": "4.17.14",
		"@types/express-http-proxy": "1.6.3",
		"@types/jsonwebtoken": "8.5.9",
		"@types/mocha": "10.0.1",
		"@types/node": "18.11.10",
		"@types/superagent": "4.1.16",
		"@types/swagger-jsdoc": "6.0.1",
		"@types/swagger-ui-express": "4.1.3",
		"@typescript-eslint/eslint-plugin": "5.45.0",
		"@typescript-eslint/parser": "5.45.0",
		"chai": "4.3.7",
		"cross-env": "7.0.3",
		"eslint": "8.29.0",
		"eslint-plugin-no-loops": "0.3.0",
		"mocha": "10.1.0",
		"nyc": "15.1.0",
		"sonar-scanner": "3.1.0",
		"start-server-and-test": "1.14.0",
		"typescript": "4.9.3"
	}
}
