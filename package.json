{
	"name": "ts-bemplate-backend",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start:ci": "node ./build/main/index.js",
		"start": "nodemon ./build/main/index.js",
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "eslint . --ext .ts",
		"lint:json": "eslint . --ext .ts -f json -o .eslint-results.json",
		"lint:fix": "eslint . --fix --ext .ts",
		"sonar": "sonar-scanner",
		"prestart": "copyfiles -f src/assets/* build/assets",
		"postbuild": "copyfiles -f src/assets/* build/assets",
		"test": "PORT=1337 start-server-and-test start:ci http://127.0.0.1:1337/documentation test:ci",
		"test:ci": "nyc mocha --require ts-node/register --extension spec.ts --recursive ./src"
	},
	"dependencies": {
		"@types/webrtc": "0.0.42",
		"bcrypt": "5.1.1",
		"bignumber.js": "9.1.2",
		"copyfiles": "2.4.1",
		"cors": "2.8.5",
		"express": "4.18.2",
		"express-http-proxy": "2.0.0",
		"jsonwebtoken": "9.0.2",
		"moment": "2.30.1",
		"nodemon": "3.1.0",
		"pg": "8.11.3",
		"reflect-metadata": "0.2.1",
		"sequelize": "6.37.1",
		"sequelize-cli": "6.6.2",
		"sequelize-typescript": "2.1.6",
		"superagent": "8.1.2",
		"swagger-jsdoc": "6.2.8",
		"swagger-ui-express": "5.0.0",
		"ts-node": "10.9.2",
		"umzug": "3.7.0",
		"uuidv4": "6.2.13",
		"web3": "4.5.0"
	},
	"devDependencies": {
		"@types/bcrypt": "5.0.2",
		"@types/chai": "4.3.12",
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/express-http-proxy": "1.6.6",
		"@types/jsonwebtoken": "9.0.6",
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.20",
		"@types/superagent": "8.1.4",
		"@types/swagger-jsdoc": "6.0.4",
		"@types/swagger-ui-express": "4.1.6",
		"@typescript-eslint/eslint-plugin": "7.1.0",
		"@typescript-eslint/parser": "7.1.0",
		"chai": "5.1.0",
		"cross-env": "7.0.3",
		"eslint": "8.57.0",
		"eslint-plugin-no-loops": "0.3.0",
		"mocha": "10.3.0",
		"nyc": "15.1.0",
		"sonar-scanner": "3.1.0",
		"start-server-and-test": "2.0.3",
		"typescript": "5.3.3"
	}
}
